{
  "name": "kitten",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --fix --ext .ts,.tsx src/",
    "prettier": "prettier --check src/**/*.tsx --write",
    "postinstall": "npx husky install && npx patch-package",
    "android:dev": "run-s env:dev && react-native run-android",
    "ios:dev": "run-s env:dev && react-native run-ios",
    "ios:11:dev": "run-s env:dev && react-native run-ios --simulator='iPhone 11 Pro'",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "pod-m1": "cd ios && arch -x86_64 pod install --repo-update && cd ..",
    "emu": "emulator -avd Pixel30",
    "clean": "react-native-clean-project",
    "clean:ios": "rm -rf ios/build",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "build:android": "yarn clean:android && cd android && ./gradlew assembleRelease && cd ..",
    "xcode": "xed ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "rename-app": "npx react-native-rename \"Name App\" -b com.nameapp",
    "env:dev": "node scripts/set-environment.js development",
    "env:stage": "node scripts/set-environment.js staging",
    "env:prod": "node scripts/set-environment.js production",
    "convert2Webp": "sh ./scripts/cwebp.sh",
    "gen-svg": "npx svgicon-create",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run eslint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.1.5",
    "@gorhom/portal": "^1.0.12",
    "@hookstate/core": "^3.0.13",
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.6.1",
    "ahooks": "^3.1.0",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "hookstate-persist": "^1.0.2",
    "i18next": "21.4.2",
    "lodash": "^4.17.21",
    "postcss": "^8.4.5",
    "rc-field-form": "^1.22.1",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-fast-compare": "^3.2.0",
    "react-i18next": "11.11.1",
    "react-native": "0.65.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.5.6",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.1.6",
    "react-native-reanimated": "^2.3.1",
    "react-native-redash": "^16.2.3",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "tailwindcss": "^2.2.19",
    "twrnc": "^2.2.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^13.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/devtools": "^6.0.4",
    "@td-design/svgicon-cli": "^1.5.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.173",
    "@types/node": "^17.0.2",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^2.2.4",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.14.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "react-dom": "^17.0.2",
    "react-native-clean-project": "^3.6.7",
    "react-native-codegen": "^0.0.12",
    "react-native-flipper": "^0.127.0",
    "react-native-rename": "^2.9.0",
    "react-test-renderer": "17.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
